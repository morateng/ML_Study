    "from typing import Tuple\n",
    "import math\n",
    "\n",
    "def normal_appoximation_to_binomial(n: int, p: float) -> Tuple[float, float]:\n",
    "    mu = p*n\n",
    "    sigma = math.sqrt(p*(1-p)*n)\n",
    "    return mu, sigma"
